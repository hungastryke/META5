package com.webdevils.utils {		public class ReverseIterator implements IIterator {		private var _data:Array;		private var _index:uint;				public function ReverseIterator( array:Array ) {			_data = array;			_index = _data.length - 1;		}				public function reset():void {			_index = _data.length - 1;		}				public function hasNext():Boolean {			return _index >= 0;		}				public function next():Object {			return Object( _data[ _index --  ] );		}	}}