package com.webdevils.utils {		public class BasicIterator implements IIterator {		private var _data:Array;		private var _index:uint;				public function BasicIterator( array:Array ) {			_data = array;			_index = 0;		}				public function reset():void {			_index = 0;		}				public function hasNext():Boolean {			return _index < _data.length;		}				public function next():Object {			return Object( _data[ _index ++  ] );		}	}}