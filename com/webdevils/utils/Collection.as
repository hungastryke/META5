package com.webdevils.utils {	public class Collection implements ICollection {		private var _data:Array;				public function Collection() {			_data = new Array();		}				public function addElement( obj:Object ):void {			_data.push( obj );		}				public function getItem( index:uint ):Object {			return Object( _data[ index ] );		}				public function get length():uint {			return _data.length;		}				public function iterator( type:String = null ):IIterator {			if ( type == "reverse" ) {				return new ReverseIterator( _data );			} else if ( type == "random" ) {				return new RandomIterator( _data );			} else {				return new BasicIterator( _data );			}		}			} // ----------------------------------------------------------}