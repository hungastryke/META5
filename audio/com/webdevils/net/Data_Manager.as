// Need to add Error checking for XMLpackage com.webdevils.net {	import flash.events.*;	import com.webdevils.net.XMLLoader;	import com.webdevils.utils.*;		public class Data_Manager extends EventDispatcher implements ICollection {		private var target_url:String;		public var _data:Collection;				public function Data_Manager( _url:String = null ) {			if ( _url != null ) {				load( _url );			}		}				public function load( _url:String ):void {			target_url = _url;			var my_xml = new XMLLoader( target_url );			my_xml.addEventListener( Event.COMPLETE, on_complete );		}				public function parse_xml_from_string( xmlString:String ):void {			var _xml = new XML( xmlString ).children();			parse_xml( _xml );		}				public function iterator( type:String = null ):IIterator {			return _data.iterator( type );		}				private function on_complete( e:Event ):void {			var _xml = new XML( e.target.data ).children();			parse_xml( _xml );		}				private function parse_xml( _xml:XMLList ):void {			_data = new Collection();						for ( var p in _xml ) {				var _obj = new Object();				var nodes = _xml[p].children();				if ( nodes.length() == 1 ) {					_obj[ nodes.name().toString() ] = nodes.toString();				} else {					for ( var q in nodes ) {						_obj[ nodes[q].name().toString() ] = nodes[q].toString();					}				}				_data.addElement( _obj );			}						dispatchEvent( new Event( Event.COMPLETE ) );		}	}}