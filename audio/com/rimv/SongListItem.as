package com.rimv {		public class SongListItem {		private var _song_name:String;		private var _song_number:uint;		private var _song_url:String;				public function SongListItem( song_name:String, song_number:uint, song_url:String ) {			_song_name = song_name;			_song_number = song_number;			_song_url = song_url;		}				// SongListItem.song_name		public function get song_name():String {			return _song_name; 		}				public function get song_number():uint {			return _song_number; 		}				public function get song_url():String {			return _song_url; 		}	}}